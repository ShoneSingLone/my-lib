{
  "useImportVue": true,
  "name": "import-vue",
  "displayName": "importVue",
  "description": "一个可以解决路径别名提示，跳转的vscode插件",
  "version": "0.0.1",
  "publisher": "ShoneSingLone",
  "engines": {
    "vscode": "^1.30.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:vue"
  ],
  "main": "./out/extension",
  "readme": "",
  "contributes": {
    "configuration": {
      "title": "importVue",
      "properties": {
        "importVue.aliasMap": {
          "type": "object",
          "description": "在这个对象中定制path-alias的规则,你可以使用${cwd}代表当前工作目录的绝对路径"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "format": "prettier"
  },
  "devDependencies": {
    "@types/chokidar": "^2.1.3",
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.10.25",
    "@types/typescript": "^2.0.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.8.0",
    "vscode": "^1.1.21",
    "prettier": "^2.3.0"
  },
  "repository": {},
  "homepage": "",
  "dependencies": {
    "typescript": "^4.2.4",
    "chokidar": "^3.4.3"
  }
}